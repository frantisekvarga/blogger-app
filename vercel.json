{
  "version": 2,
  "builds": [
    {
      "src": "apps/app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/apps/app"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
